@if(TempData["success"]!=null){
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            if (window.Swal) {
                Swal.fire({
                    icon: 'success',
                    title: 'Success',
                    text: '@TempData["success"]',
                    timer: 1500,
                    showConfirmButton: false
                });
            }
        });
    </script>
}
@if (TempData["error"]!= null){
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            if (window.Swal) {
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: '@TempData["error"]'
                });
            }
        });
    </script>
}